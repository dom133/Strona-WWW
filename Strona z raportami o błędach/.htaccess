### PROTECT FILES ###
<FilesMatch "\.(htaccess|htpasswd|ini|fla|psd|log|sh)$">
Order Allow,Deny
Deny from all
</FilesMatch>

RewriteEngine on

RewriteRule dodaj index.php?action=dodaj
RewriteRule logowanie index.php?action=login
RewriteRule rejestracja index.php?action=rejestracja

AddHandler application/x-httpd-php55 .php #wersja_php_generowane_automatycznie